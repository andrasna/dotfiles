call plug#begin('~/.vim/plugged')

Plug 'yuezk/vim-js'
Plug 'maxmellon/vim-jsx-pretty'
Plug 'gosukiwi/vim-atom-dark'
Plug 'editorconfig/editorconfig-vim'

call plug#end()

set nocompatible

set encoding=utf-8

set hlsearch

set tabstop=2

set shiftwidth=2

set expandtab

set autoindent

set number 

set showmode

set showcmd

set modeline

set ruler

set title

set ignorecase

set smartcase

set mouse=a

set nocp

syntax on

set background=dark

set t_Co=256

set cursorline

colorscheme atom-dark-256 

